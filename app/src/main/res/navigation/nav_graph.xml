<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/ListingFragment">

    <fragment
        android:id="@+id/ListingFragment"
        android:name="com.brandonkitt.alarms.view.ListingFragment"
        android:label="@string/listing_fragment_label"
        tools:layout="@layout/fragment_listing">

        <action
            android:id="@+id/action_ListingFragment_to_DetailsFragment"
            app:destination="@id/DetailsFragment" />
    </fragment>

    <fragment
        android:id="@+id/DetailsFragment"
        android:name="com.brandonkitt.alarms.view.DetailsFragment"
        android:label="@string/details_fragment_label"
        tools:layout="@layout/fragment_details">

        <action
            android:id="@+id/action_DetailsFragment_to_ListingFragment"
            app:destination="@id/ListingFragment" />
    </fragment>
</navigation>